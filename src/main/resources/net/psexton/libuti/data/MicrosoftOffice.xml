    Description:
    
    UTIs for Microsoft Office documents
-->

<uti-list>
    <!-- Non-XML Versions -->
    
    <uti>
        <name>com.microsoft.word.doc</name>
        <suffix preferred="true">doc</suffix>
        <conforms-to>public.data</conforms-to>
        <conforms-to>public.composite-content</conforms-to>
    </uti>
    
    <uti>
        <name>com.microsoft.excel.xls</name>
        <suffix preferred="true">xls</suffix>
        <conforms-to>public.data</conforms-to>
        <conforms-to>public.composite-content</conforms-to>
    </uti>
    
    <uti>
        <name>com.microsoft.powerpoint.ppt</name>
        <suffix preferred="true">ppt</suffix>
        <conforms-to>public.data</conforms-to>
        <conforms-to>public.presentation</conforms-to>
    </uti>
    
    <uti>
        <name>com.microsoft.powerpoint.pps</name>
        <suffix preferred="true">pps</suffix>
        <conforms-to>public.data</conforms-to>
        <conforms-to>public.presentation</conforms-to>
    </uti>
    
    <!-- Template forms of non-XML Versions -->
    
    <uti>
        <name>com.microsoft.word.dot</name>
        <suffix preferred="true">dot</suffix>
        <conforms-to>public.data</conforms-to>
        <conforms-to>public.composite-content</conforms-to>
    </uti>
    
    <uti>
        <name>com.microsoft.excel.xlt</name>
        <suffix preferred="true">xlt</suffix>
        <conforms-to>public.data</conforms-to>
        <conforms-to>public.composite-content</conforms-to>
    </uti>
    
    <uti>
        <name>com.microsoft.powerpoint.pot</name>
        <suffix preferred="true">pot</suffix>
        <conforms-to>public.data</conforms-to>
        <conforms-to>public.presentation</conforms-to>
    </uti>
    
    <!-- Microsoft Office 2007 (XML versions) -->
    
    <uti>
        <name>com.microsoft.word.docx</name>
        <suffix preferred="true">docx</suffix>
        <suffix>docm</suffix>
        <conforms-to>public.data</conforms-to>
        <conforms-to>public.composite-content</conforms-to>
    </uti>
    
    <uti>
        <name>com.microsoft.excel.xlsx</name>
        <suffix preferred="true">xlsx</suffix>
        <suffix>xlsm</suffix>
        <conforms-to>public.data</conforms-to>
        <conforms-to>public.composite-content</conforms-to>
    </uti>
    
    <uti>
        <name>com.microsoft.powerpoint.pptx</name>
        <suffix preferred="true">pptx</suffix>
        <suffix>pptm</suffix>
        <conforms-to>public.data</conforms-to>
        <conforms-to>public.presentation</conforms-to>
    </uti>
    
    <uti>
        <name>com.microsoft.powerpoint.ppsx</name>
        <suffix preferred="true">ppsx</suffix>
        <conforms-to>public.data</conforms-to>
        <conforms-to>public.presentation</conforms-to>
    </uti>
    
    <!-- Template forms of XML Versions -->
    
    <uti>
        <name>com.microsoft.word.dotx</name>
        <suffix preferred="true">dotx</suffix>
        <conforms-to>public.data</conforms-to>
        <conforms-to>public.composite-content</conforms-to>
    </uti>
    
    <uti>
        <name>com.microsoft.excel.xltx</name>
        <suffix preferred="true">xltx</suffix>
        <conforms-to>public.data</conforms-to>
        <conforms-to>public.composite-content</conforms-to>
    </uti>
    
    <uti>
        <name>com.microsoft.powerpoint.potx</name>
        <suffix preferred="true">potx</suffix>
        <conforms-to>public.data</conforms-to>
        <conforms-to>public.presentation</conforms-to>
    </uti>
    
</uti-list>
