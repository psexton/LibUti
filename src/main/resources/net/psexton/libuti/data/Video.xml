<uti-list>

    <uti>
        <name>com.apple.quicktime-movie</name>
        <suffix preferred="true">mov</suffix>
        <suffix>qt</suffix>
        <conforms-to>public.movie</conforms-to>
    </uti>
    
    <uti>
        <name>public.avi</name>
        <suffix preferred="true">avi</suffix>
        <conforms-to>public.movie</conforms-to>
    </uti>
    
    <uti>
        <name>public.mpeg</name>
        <suffix>mpg</suffix>
        <suffix preferred="true">mpeg</suffix>
        <suffix>m75</suffix>
        <suffix>m15</suffix>
        <conforms-to>public.movie</conforms-to>
    </uti>
    
    <uti>
        <name>public.mpeg-4</name>
        <suffix preferred="true">mp4</suffix>
        <conforms-to>public.movie</conforms-to>
    </uti>
    
    <uti>
        <name>com.microsoft.advanced-systems-format</name>
        <suffix preferred="true">asf</suffix>
        <conforms-to>public.audiovisual-content</conforms-to>
    </uti>
    
    <uti>
        <name>com.microsoft.windows-media-wmv</name>
        <suffix preferred="true">wmv</suffix>
        <conforms-to>public.movie</conforms-to>
        <conforms-to>com.microsoft.advanced-systems-format</conforms-to>
    </uti>
    
</uti-list>

