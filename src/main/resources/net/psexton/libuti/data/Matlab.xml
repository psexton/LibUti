<uti-list>
    
    <uti>
        <name>com.mathworks.matlab-code</name>
        <suffix preferred="true">m</suffix>
        <conforms-to>public.source-code</conforms-to>
    </uti>
    
    <uti>
        <name>com.mathworks.matlab-data</name>
        <suffix preferred="true">mat</suffix>
        <conforms-to>public.composite-content</conforms-to>
    </uti>
    
    <uti>
        <name>com.mathworks.matlab-figure</name>
        <suffix preferred="true">fig</suffix>
        <conforms-to>public.composite-content</conforms-to>
    </uti>
    
</uti-list>
