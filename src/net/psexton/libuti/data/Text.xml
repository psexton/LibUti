<!--
    Document   : Text.xml
    Created on : November 28, 2011, 3:10 PM
    Author     : PSexton
    Description:
        UTI for text and text-like documents.
-->

<uti-list>

    <uti>
        <name>public.plain-text</name>
        <suffix preferred="true">txt</suffix>
        <suffix>text</suffix>
        <conforms-to>public.text</conforms-to>
    </uti>
    
    <uti>
        <name>public.rtf</name>
        <suffix preferred="true">rtf</suffix>
        <conforms-to>public.text</conforms-to>
    </uti>
    
    <uti>
        <name>public.html</name>
        <suffix>htm</suffix>
        <suffix preferred="true">html</suffix>
        <conforms-to>public.text</conforms-to>
    </uti>
    
    <uti>
        <name>public.xml</name>
        <suffix preferred="true">xml</suffix>
        <conforms-to>public.text</conforms-to>
    </uti>
    
    <uti>
        <name>public.comma-separated-values-text</name>
        <suffix preferred="true">csv</suffix>
        <conforms-to>public.text</conforms-to>
    </uti>
    
    <uti>
        <name>public.css</name>
        <suffix preferred="true">css</suffix>
        <conforms-to>public.text</conforms-to>
    </uti>
    
    <uti>
        <name>public.markdown</name>
        <suffix preferred="true">md</suffix>
        <suffix>markdown</suffix>
        <conforms-to>public.text</conforms-to>
    </uti>
    
    <uti>
        <name>public.yaml</name>
        <suffix preferred="true">yaml</suffix>
        <suffix>yml</suffix>
        <conforms-to>public.text</conforms-to>
    </uti>
    
    <uti>
        <name>public.json</name>
        <suffix preferred="true">json</suffix>
        <conforms-to>public.text</conforms-to>
    </uti>
    
</uti-list>