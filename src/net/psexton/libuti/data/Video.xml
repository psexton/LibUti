<uti-list>

    <uti>
        <name>com.apple.quicktime-movie</name>
        <suffix>mov</suffix>
        <suffix>qt</suffix>
        <conforms-to>public.movie</conforms-to>
    </uti>
    
    <uti>
        <name>public.avi</name>
        <suffix>avi</suffix>
        <conforms-to>public.movie</conforms-to>
    </uti>
    
    <uti>
        <name>public.mpeg</name>
        <suffix>mpg</suffix>
        <suffix>mpeg</suffix>
        <suffix>m75</suffix>
        <suffix>m15</suffix>
        <conforms-to>public.movie</conforms-to>
    </uti>
    
    <uti>
        <name>public.mpeg-4</name>
        <suffix>mp4</suffix>
        <conforms-to>public.movie</conforms-to>
    </uti>
    
</uti-list>

