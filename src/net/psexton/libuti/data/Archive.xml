    Description:
        UTIs for archive file formats.
-->

<uti-list>
    
    <!-- TAR -->
    <uti>
        <name>org.gnu.gnu-tar-archive</name>
        <suffix preferred="true">gtar</suffix>
        <conforms-to>public.data</conforms-to>
        <conforms-to>public.archive</conforms-to>
    </uti>
    <uti>
        <name>public.tar-archive</name>
        <suffix preferred="true">tar</suffix>
        <conforms-to>org.gnu.gnu-tar-archive</conforms-to>
    </uti>
    
    <!-- ZIP -->
    <uti>
        <name>com.pkware.zip-archive</name>
        <suffix preferred="true">zip</suffix>
        <conforms-to>public.data</conforms-to>
        <conforms-to>public.archive</conforms-to>
    </uti>
    
    <!-- GZIP -->
    <uti>
        <name>org.gnu.gnu-zip-archive</name>
        <suffix preferred="true">gz</suffix>
        <suffix>gzip</suffix>
        <conforms-to>public.data</conforms-to>
        <conforms-to>public.archive</conforms-to>
    </uti>
    
    <!-- TARRED GZIP -->
    <uti>
        <name>org.gnu.gnu-zip-tar-archive</name>
        <suffix preferred="true">tgz</suffix>
        <conforms-to>org.gnu.gnu-zip-archive</conforms-to>
    </uti>
    
</uti-list>
