<uti-list>
    
    <!-- Adobe -->
    
    <uti>
        <name>com.adobe.pdf</name>
        <suffix preferred="true">pdf</suffix>
        <conforms-to>public.data</conforms-to>
        <conforms-to>public.composite-content</conforms-to>
    </uti>
    
    <uti>
        <name>com.adobe.postscript</name>
        <suffix preferred="true">ps</suffix>
        <conforms-to>public.data</conforms-to>
    </uti>
    
    <!-- Executables -->
    
    <uti>
        <name>com.microsoft.windows-executable</name>
        <suffix preferred="true">exe</suffix>
        <conforms-to>public.data</conforms-to>
        <conforms-to>public.executable</conforms-to>
    </uti>
    
    <uti>
        <name>com.microsoft.windows-dynamic-link-library</name>
        <suffix preferred="true">dll</suffix>
        <conforms-to>public.data</conforms-to>
        <conforms-to>public.executable</conforms-to>
    </uti>
    
    <uti>
        <name>com.sun.java-archive</name>
        <suffix preferred="true">jar</suffix>
        <conforms-to>public.data</conforms-to>
        <conforms-to>public.executable</conforms-to>
        <conforms-to>public.archive</conforms-to>
    </uti>
    
    <!-- Programming -->
    
    <uti>
        <name>public.shell-script</name>
        <suffix preferred="true">sh</suffix>
        <suffix>command</suffix>
        <conforms-to>public.script</conforms-to>
    </uti>
    
    <uti>
        <name>public.csh-script</name>
        <suffix preferred="true">csh</suffix>
        <conforms-to>public.shell-script</conforms-to>
    </uti>
    
    <uti>
        <name>public.python-script</name>
        <suffix preferred="true">py</suffix>
        <conforms-to>public.shell-script</conforms-to>
    </uti>
    
    <uti>
        <name>public.ruby-script</name>
        <suffix preferred="true">rb</suffix>
        <suffix>rbw</suffix>
        <conforms-to>public.shell-script</conforms-to>
    </uti>
    
</uti-list>
