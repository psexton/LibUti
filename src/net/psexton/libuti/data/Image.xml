<uti-list>

    <uti>
        <name>public.jpeg</name>
        <suffix>jpg</suffix>
        <suffix>jpeg</suffix>
        <conforms-to>public.image</conforms-to>
    </uti>
    
    <uti>
        <name>public.tiff</name>
        <suffix>tif</suffix>
        <suffix>tiff</suffix>
        <conforms-to>public.image</conforms-to>
    </uti>
    
    <uti>
        <name>public.png</name>
        <suffix>png</suffix>
        <conforms-to>public.image</conforms-to>
    </uti>
    
    <uti>
        <name>com.compuserve.gif</name>
        <suffix>gif</suffix>
        <conforms-to>public.image</conforms-to>
    </uti>
    
    <uti>
        <name>com.microsoft.bmp</name>
        <suffix>bmp</suffix>
        <conforms-to>public.image</conforms-to>
    </uti>
    
</uti-list>
