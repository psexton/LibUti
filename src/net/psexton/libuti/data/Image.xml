<uti-list>

    <uti>
        <name>public.jpeg</name>
        <suffix>jpg</suffix>
        <suffix preferred="true">jpeg</suffix>
        <conforms-to>public.image</conforms-to>
    </uti>
    
    <uti>
        <name>public.tiff</name>
        <suffix>tif</suffix>
        <suffix preferred="true">tiff</suffix>
        <conforms-to>public.image</conforms-to>
    </uti>
    
    <uti>
        <name>public.png</name>
        <suffix preferred="true">png</suffix>
        <conforms-to>public.image</conforms-to>
    </uti>
    
    <uti>
        <name>com.compuserve.gif</name>
        <suffix preferred="true">gif</suffix>
        <conforms-to>public.image</conforms-to>
    </uti>
    
    <uti>
        <name>com.microsoft.bmp</name>
        <suffix preferred="true">bmp</suffix>
        <conforms-to>public.image</conforms-to>
    </uti>
    
</uti-list>
